{
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "jsx",
    "js",
    "css"
  ],
  "html.format.indentInnerHtml": true,
  "html.autoClosingTags": true,
  "sh.enable": true,
  "rest-client.enable": true,
  "rest-client.showHeaders": true
}
